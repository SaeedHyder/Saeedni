            android:layout_width="match_parent"
            android:layout_height="@dimen/x40"
            android:layout_marginTop="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:focusableInTouchMode="true"
            android:background="@drawable/boxcentershape">

            <android.support.v7.widget.SearchView
                android:id="@+id/edtSearch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:hint="@string/searchService"
                android:focusedByDefault="false"
                android:imeOptions="actionDone"
                android:queryHint="@string/searchService"
                app:closeIcon="@null"
                app:iconifiedByDefault="false"

                app:queryBackground="@color/transparent"
                app:queryHint="@string/searchService"
                app:searchIcon="@null" />
        </RelativeLayout>

        <com.ingic.saeedni.ui.views.CustomRecyclerView
            android:id="@+id/rvJobs"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>


</LinearLayout>

